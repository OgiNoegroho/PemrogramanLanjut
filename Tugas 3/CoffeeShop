package TugasInheritance;

//Superclass
public class CoffeShop {

    // Attribut
    private String name;
    private int price;

    //Konstruktor
    public CoffeShop (){};
    public CoffeShop (String name, int price){
        this.name = name;
        this.price = price;
    }
    //method
    //menggunakan get and set untuk memanggil attribut yang memiliki modifier private
    public String getName(){
        return name;
    }
    public void setName(String name){
        this.name = name;
    }

    public int getPrice(){
        return price;
    }

    public void setPrice(int price){
        this.price = price;
    }
}

