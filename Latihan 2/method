package latihan2;

/**
 *
 * @author Lenovo
 */
public class method {

  
    public static void main(String[] args) { 
        BengkelMotorBerkah method = new BengkelMotorBerkah("YAMAHA","Aerox",2021);
        BengkelMotorBerkah method2 = new BengkelMotorBerkah();
        
     
        method.Servis();
        method.GantiOli();
        System.out.println(method.Selesai());
        
        method2.Merek("YAMAHA");
        method2.Nama("Aerox");
        method2.TahunProduksi(2021);
        method2.Servis();
        method2.GantiOli();
        System.out.println(method2.Selesai());
    }
    
    
}
